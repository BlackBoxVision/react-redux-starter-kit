{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "lodash"
    ],
    "env": {
        "production": {
            "presets": [
                "es2015",
                "react",
                "react-optimize",
                "stage-0"
            ],
            "plugins": [
                "transform-decorators-legacy",
                "transform-object-rest-spread",
                "transform-react-remove-prop-types"
            ]
        }
    }
}