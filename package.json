{
  "author": "BlackBox Vision",
  "name": "react-redux-starter-kit",
  "description": "Universal boilerplate with React/Redux under Atomic Design approach",
  "version": "1.0.1",
  "repository": {
    "url": "https://github.com/BlackBoxVision/react-redux-starter-kit.git",
    "type": "git"
  },
  "license": "MIT",
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": "6.9.1",
    "npm": "3.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "webpack-app:dev": "better-npm-run webpack-app:dev",
    "webpack-app:prod": "better-npm-run webpack-app:prod",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "start": "yarn run build:prod",
    "start:dev": "yarn run build:dev"
  },
  "betterScripts": {
    "build:dev": {
      "command": "yarn run clean && yarn install && yarn run webpack-app:dev && node index.js",
      "env": {
        "BABEL_DISABLE_CACHE": "1",
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "yarn run clean && yarn run webpack-app:prod && node index.js",
      "env": {
        "BABEL_DISABLE_CACHE": "1",
        "NODE_ENV": "production"
      }
    },
    "webpack-app:dev": {
      "command": "parallel-webpack --config webpack.config.dev.js"
    },
    "webpack-app:prod": {
      "command": "parallel-webpack -p --optimize-minimize --config webpack.config.js"
    }
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "helmet": "^3.0.0",
    "helmet-csp": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.0.1",
    "styled-components": "^2.2.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.6",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "babili-webpack-plugin": "0.0.11",
    "better-npm-run": "^0.0.14",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "js-polyfills": "^0.1.27",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.4",
    "smoothscroll-polyfill": "^0.3.4",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-externals-plugin": "^1.0.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
